{
  "name": "lambda-angular-universal",
  "version": "0.0.1",
  "description": "Template for creating Angular 5 universal apps atop Lambda",
  "main": "lambda.js",
  "scripts": {
    "test-var" : "source .env && echo $LAU_STACK_NAME",
    "ng": "ng",
    "build": "ng build --prod",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "build-full": "npm run build-client-and-server-bundles && npm run webpack-server && cp src/run-sample-event.js dist && cp src/sample-api-gateway-event.json dist",
    "build-lambda": "npm run build-client-and-server-bundles && npm run webpack-server && cd dist && cp ../package.json . && npm install --production && rm package.json && cd ..",
    "clean": "rm -Rf dist",
    "fix-bad-uglify-dates": "touch -t 198001010000 /tmp/1980-Jan-01-0000 && find ./dist -type f ! -newer /tmp/1980-Jan-01-0000 -exec touch -t 201601011200 '{}' \\;",
    "build-client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack-server": "webpack --config webpack.server.config.js --progress --colors",
    "package": "source .env && npm run clean && npm run build-lambda && npm run fix-bad-uglify-dates && aws cloudformation package --template ./cloudformation.yaml --s3-bucket $LAU_S3_BUCKET --output-template packaged-sam.yaml --region $LAU_AWS_REGION",
    "deploy": "source .env && aws cloudformation deploy --template-file packaged-sam.yaml --stack-name $LAU_STACK_NAME --capabilities CAPABILITY_IAM --region $LAU_AWS_REGION",
    "package-deploy": "source .env && npm run package && npm run deploy",
    "delete-stack": "source .env && aws cloudformation delete-stack --stack-name $LAU_STACK_NAME --region $LAU_AWS_REGION",
    "setup": "source .env && npm install && (aws s3 get-bucket-location --bucket $LAU_S3_BUCKET --region $LAU_AWS_REGION || npm run create-bucket) && npm run package-deploy"
  },
  "keywords": [
    "angular",
    "lambda",
    "serverless",
    "universal"
  ],
  "author": "Christopher Weiss <bitblit@gmail.com>",
  "bugs": {
    "url": "https://github.com/bitblit/LambdaAngularUniversal/issues"
  },
  "homepage": "https://github.com/bitblit/LambdaAngularUniversal#readme",
  "engines": {
    "node": ">=8.10.2"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "~5.2.9",
    "@angular/common": "~5.2.9",
    "@angular/compiler": "~5.2.9",
    "@angular/compiler-cli": "~5.2.9",
    "@angular/core": "~5.2.9",
    "@angular/forms": "~5.2.9",
    "@angular/http": "~5.2.9",
    "@angular/platform-browser": "~5.2.9",
    "@angular/platform-browser-dynamic": "~5.2.9",
    "@angular/platform-server": "~5.2.9",
    "@angular/router": "~5.2.9",
    "@angular/upgrade": "~5.2.9",
    "@bitblit/ratchet": "0.0.52",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "angular-in-memory-web-api": "~0.5.0",
    "core-js": "^2.4.1",
    "mime-types": "^2.1.18",
    "rxjs": "^5.5.0",
    "ts-loader": "^3.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@types/aws-lambda": "0.0.31",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/debug": "0.0.29",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "ajv": "^6.0.0",
    "aws-lambda-typescript": "^1.0.2",
    "aws-sdk": "^2.195.0",
    "hoek": "~4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.1.0",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "typescript": "2.6.2"
  },
  "repository": {}
}

{
  "name": "lambda-angular-universal",
  "version": "0.0.1",
  "description": "Template for creating Angular 5 universal apps atop Lambda",
  "main": "lambda.js",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "extract": "ng xi18n --outputPath=src/locale",
    "build:full": "npm run build:client-and-server-bundles && npm run webpack:server && gulp build:full",
    "build:lambda": "npm run build:client-and-server-bundles && npm run webpack:server && gulp build:lambda",
    "clean": "gulp clean",
    "serve:full": "cd dist && node server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js",
    "local": "node scripts/local",
    "invoke:lambda": "gulp invoke:lambda",
    "create:bucket": "gulp create:bucket",
    "delete:bucket": "gulp delete:bucket",
    "package:cloudformation": "npm run clean && npm run build:lambda && gulp package:cloudformation",
    "deploy:cloudformation": "gulp deploy:cloudformation",
    "package-deploy": "npm run package:cloudformation && npm run deploy:cloudformation",
    "delete:stack": "gulp delete:stack",
    "setup": "npm install && (gulp setup  || gulp create:bucket) && npm run package-deploy"
  },
  "keywords": [
    "angular",
    "lambda",
    "serverless",
    "universal"
  ],
  "author": "Christopher Weiss <bitblit@gmail.com>",
  "bugs": {
    "url": "https://github.com/bitblit/LambdaAngularUniversal/issues"
  },
  "homepage": "https://github.com/bitblit/LambdaAngularUniversal#readme",
  "engines": {
    "node": ">=6.10.2"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "~5.2.9",
    "@angular/common": "~5.2.9",
    "@angular/compiler": "~5.2.9",
    "@angular/compiler-cli": "~5.2.9",
    "@angular/core": "~5.2.9",
    "@angular/forms": "~5.2.9",
    "@angular/http": "~5.2.9",
    "@angular/platform-browser": "~5.2.9",
    "@angular/platform-browser-dynamic": "~5.2.9",
    "@angular/platform-server": "~5.2.9",
    "@angular/router": "~5.2.9",
    "@angular/upgrade": "~5.2.9",
    "@bitblit/ratchet": "0.0.19",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "angular-in-memory-web-api": "~0.5.0",
    "aws-serverless-express": "^3.0.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "morgan": "^1.7.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4",
    "ts-loader": "^3.1.1"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@types/aws-lambda": "0.0.31",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.33",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/mocha": "^2.2.32",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.45",
    "ajv": "^6.0.0",
    "aws-lambda-typescript": "^1.0.2",
    "aws-sdk": "^2.195.0",
    "del": "^3.0.0",
    "dotenv": "^5.0.1",
    "gulp": "^3.9.1",
    "gulp-install": "^1.1.0",
    "gulp-replace": "^0.6.1",
    "gulp-run-command": "0.0.9",
    "gulp-touch-fd": "github:funkedigital/gulp-touch-fd",
    "hoek": "~4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.1.0",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "typescript": "2.6.2"
  },
  "repository": {}
}
